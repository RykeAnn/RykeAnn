                            height: self.texture_size[1]
                            text_size: self.width, None
                            padding: 10, 10
                            text: '             '
                            font_size:'17sp'
                            color: 0,0,0

                        Label:
                            Image:
                                center: self.parent.center
                                source: '14.jpg'
                                width: 300
                                height: 150

                        Label:
                            size_hint_y: None
                            height: self.texture_size[1]
                            text_size: self.width, None
                            padding: 10, 10
                            text: '             '
                            font_size:'17sp'
                            color: 0,0,0

                        Label:
                            size_hint_y: None
                            height: self.texture_size[1]
                            text_size: self.width, None
                            padding:10, 5
                            text: '         Use "tags" in Windows 10 to keep track of files if you are constantly having difficulties finding them on your computer. Simply right-click a file and select Properties. Then go to the Details page, and if you click on the Tags section (under Description), you can add a tag from there. Those tags can then be used when searching for information - for example, when you tag a few files as a specific work project, they will all show up when you search for that phrase in the future.'
                            color: (244,164,96)

                        Label:
                            size_hint_y: None
                            height: self.texture_size[1]
                            text_size: self.width, None
                            padding: 10, 10
                            text: '15. Using Windows 10 gestures to work quickly'
