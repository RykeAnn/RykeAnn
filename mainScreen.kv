                            halign: 'center'
                            bold: True
                            color: (244,164,96)

                        Label:
                            size_hint_y: None
                            height: self.texture_size[1]
                            text_size: self.width, None
                            padding: 10, 10
                            text:  'STEP 1'
                            font_size:'20sp'
                            bold: True
                            color: (30,144,255)
                        Label:
                            size_hint_y: None
                            height: self.texture_size[1]
                            text_size: self.width, None
                            padding: 10, 10
                            text: '             '
                            font_size:'17sp'
                            color: 0,0,0

                        Label:
                            Image:
                                center: self.parent.center
                                source: 'step 1.jpg'
