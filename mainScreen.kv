                            text_size: self.width, None
                            padding: 10, 12
                            text: '             '
                            font_size:'17sp'
                            color: 0,0,0

                        Label:
                            size_hint_y: None
                            height: self.texture_size[1]
                            text_size: self.width, None
                            padding:  10, 5
                            text: '         Windows 10 is a Microsoft operating system for personal computers, tablets, embedded devices and internet of things devices. Windows 10 features built-in capabilities that allow corporate IT departments to use mobile device management (MDM) software to secure and control devices running the operating system.'
                            color: (244,164,96)

                        Label:
                            size_hint_y: None
                            height: self.texture_size[1]
                            text_size: self.width, None
                            padding: 10, 10
                            text: 'Windows 10 features'
                            font_size: '20sp'
                            bold: True
                            color: (30,144,255)

                        Label:
                            size_hint_y: None
                            height: self.texture_size[1]
                            text_size: self.width, None
                            padding: 10, 10
                            text: '             '
