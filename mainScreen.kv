                        Label:
                            size_hint_y: None
                            height: self.texture_size[1]
                            text_size: self.width, None
                            padding: 10, 10
                            text: 'Make Start Menu and Taskbar Colorful'
                            font_size:'25sp'
                            bold: True
                            color:(30,144,255)

                        Label:
                            size_hint_y: None
                            height: self.texture_size[1]
                            text_size: self.width, None
                            padding: 10, 10
                            text: 'You must have seen some Windows pictures with unusual colors on the theme. In fact, Windows 10 enables to change the colors of your Start Menu and Taskbar. If you don’t feel like the default color Windows 10 picks for you, you can add a new custom color for Windows 10 taskbar.                                                              Step 1. Go to Windows 10 Start Menu and enter Settings > Personalization.                                                                               Step 2. Then navigate to Colors where you can check Automatically pick an accent color from background, or you can scroll down to choose your accent color.'
                            font_size:'17sp'
                            color: (244,164,96)


                        Label:
                            size_hint_y: None
                            height: self.texture_size[1]
                            text_size: self.width, None
                            padding: 10, 10
                            text: '                                             '
