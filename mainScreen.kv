                        Label:
                            size_hint_y: None
                            height: self.texture_size[1]
                            text_size: self.width, None
                            padding: 10, 10
                            text: '#1: Open a new browser window or tab.                                        *If you press F1 on Windows 10 desktop, you’ll be brought to the how to get help in windows 10 search page in Edge.                                                                                     *If you press F1 in an opening browser, you’ll get a new tab (often shows the corresponding support page).                                                                                                                       *Press Windows + F1 will also take you to the Microsoft Windows help and support center.'
                            font_size:'17sp'
                            color: (244,164,96)
                        Label:
                            size_hint_y: None
                            height: self.texture_size[1]
                            text_size: self.width, None
