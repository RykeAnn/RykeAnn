MDScreen:
    name: "MainScreen"

    ScreenManager:
        id: main_screen
        Screen:
            name:"screen1"
            BoxLayout:
                orientation: 'vertical'

                MDBottomAppBar:
                    MDToolbar:
                        title: 'explore'
                        mode: 'end'
                        type: 'bottom'
                        icon: 'microsoft-windows'
                        on_action_button:
                            main_screen.current="screen1"


            BoxLayout:
                orientation: 'vertical'
                MDToolbar:
                    id:md_toolbar
                    title: 'Intro to windows 10'
                    left_action_items: [['menu', lambda x: nav_drawer.set_state("open")]]
                    elevation: 10

                ScrollView:
                    DrawerList:
                        id: md_list
                        MDList:
                            OneLineIconListItem:
                                text: "Overview"
                                on_press:
                                    main_screen.current="md_ch1"
                                IconLeftWidget:
                                    icon: "book"

                            OneLineIconListItem:
                                text: "Different Versions of Windows 10"
                                on_press:
                                    main_screen.current="md_ch2"
                                IconLeftWidget:
                                    icon: "book"


                            OneLineIconListItem:
                                text: "How to Install"
                                on_press:
                                    main_screen.current="md_ch3"
                                IconLeftWidget:
                                    icon: "book"


                            OneLineIconListItem:
                                text: "Basic Functions"
                                on_press:
                                    main_screen.current="md_ch4"
                                IconLeftWidget:
                                    icon: "book"

                            OneLineIconListItem:
                                text: "Troubleshooting Tips"
                                on_press:
                                    main_screen.current="md_ch5"
                                IconLeftWidget:
                                    icon: "book"

                            OneLineIconListItem:
                                text: "Windows 10 Tricks"
                                on_press:
                                    main_screen.current="md_ch6"
                                IconLeftWidget:
                                    icon: "book"

                            OneLineIconListItem:
                                text: "Shortcut Keys"
                                on_press:
                                    main_screen.current="md_ch7"
                                IconLeftWidget:
                                    icon: "table-key"



#overview


        Screen:
            name:"md_ch1"
            BoxLayout:
                orientation: 'vertical'
