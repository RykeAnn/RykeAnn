                            text_size: self.width, None
                            padding: 10, 10
                            text: 'ABOUT WINDOWS'
                            font_size: '20sp'
                            halign: 'left'
                            color: (30,144,255)

                        Label:
                            size_hint_y: None
                            height: self.texture_size[1]
                            text_size: self.width, None
                            padding: 10, 10
                            text: '             '
                            font_size:'17sp'
